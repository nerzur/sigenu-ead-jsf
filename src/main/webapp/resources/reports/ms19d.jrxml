		<queryString>
			<![CDATA[select distinct asignatura_nombre,nota1,nota2,nota3,count(distinct asignatura_id)
from crosstab
('select distinct  asignatura_nombre,examenexamen_id,nota from asignatura inner join examen_matricula_facultad_cum_carrera_estudiante_asignatura on asignatura.asignatura_id = examen_matricula_facultad_cum_carrera_estudiante_asignatura.asignatura_id,carreranacional, carrera,examen,estudiante where carrera.carreranacionalidcarreranacional = carreranacional.idcarreranacional   and examen.examen_id = examen_matricula_facultad_cum_carrera_estudiante_asignatura.examenexamen_id and examen_matricula_facultad_cum_carrera_estudiante_asignatura.matricula_id =1  group by asignatura_nombre,examenexamen_id,nota order by asignatura_nombre,examenexamen_id,nota',
'select 1 union select 2 union select 3 order by 1')as (asignatura_nombre character varying, nota1 int,nota2 int, nota3 int),  examen_matricula_facultad_cum_carrera_estudiante_asignatura
where examen_matricula_facultad_cum_carrera_estudiante_asignatura.estudiante_id= $P{nombre1} 
