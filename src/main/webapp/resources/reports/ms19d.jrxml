(select round(avg(nota),2) from examen_matricula_facultad_cum_carrera_estudiante_asignatura where
 examen_matricula_facultad_cum_carrera_estudiante_asignatura.estudiante_id=$P{id})as avg
from estudiante inner join matricula_facultad_cum_carrera_estudiante_asignatura
on estudiante.estudiante_id = matricula_facultad_cum_carrera_estudiante_asignatura.estudiante_id, asignatura,
 carreranacional, carrera,universidad,curso,facultad
